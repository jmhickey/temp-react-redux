{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "Starter Project for React with Redux",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "generate-mock-data": "babel-node tools/generateMockData.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Jason Hickey",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chance": "^1.0.16",
    "compression": "^1.7.3",
    "css-loader": "0.25.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "4.19.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "4.0.2",
    "expect": "23.6.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fetch-mock": "^7.2.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.14.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "npm-run-all": "3.1.1",
    "opn": "^5.4.0",
    "raf": "^3.4.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
